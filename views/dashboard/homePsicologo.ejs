                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        
                        <input id="txtid" type="hidden" value="0" />
        
                        <form id="adminAddUser" class="row justify-content-center">

                            <div class="col-sm-6">
             
                              <div class="mb-3">
                                    <label class="form-label">Nombre</label>
                                    <input type="txt" class="form-control" id="nombreusuario" placeholder="Escriba su nombre" name="nombre" autocomplete="on" required/>
                              </div>
      
                              <div class="mb-3">
                                    <label class="form-label">Apellidos</label>
                                    <input type="txt" class="form-control" id="apellidousuario" placeholder="Escriba su apellido" name="apellidos" required/>
                                </div>
      
                              <div class="mb-3">
                                <label for="form-label">Fecha de nacimiento</label>
                                <input class="form-control" id="fechaNacimiento" type="date" required/>
                              </div>                    
      
                              <div class="mb-3">
                                <label for="form-label">Correo Electronico</label>
                                <input class="form-control" id="correo" type="txt" placeholder="Escriba su correo" required/>
                              </div>
      
                              <!-- <div class="mb-3">
                                <label for="form-label">Contraseña</label>
                                <input class="form-control" id="contraseña" type="password" placeholder="************" />                                  
                              </div> -->
                              <div class="mb-3">
                                <label for="form-label">ROL</label>
                                <input class="form-control" id="roleUsuario" type="txt" placeholder="Paciente" disabled/>

                                <!-- <select class="form-control" id="roleUsuario" type="txt"  required >
                                  <option value="Administrador">Administrador</option>
                                  <option value="Psicologo">Psicologo</option>  
                                  <option value="Paciente">Paciente</option>  
      
                                </select>                                -->
                              </div>                               
      
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-warning" data-bs-dismiss="modal">Cerrar</button>

                                    <input type="submit" class="btn btn-primary" name="Registrarse" value="Registrarse" onclick="sendDataAdmin()" >
                           
                            </div>

                        </form>

                    </div>

                </div>
            </div>
